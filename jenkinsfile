pipeline{
    agent any
    tools{
        nodejs 'nodejs'
    }
    stages{
        stage('Build'){
            steps{
            sh 'npm install'
            }
        }
        stage('Test'){
            steps{
            sh 'npm install' || echo "No tests defined"
            }
        }
        stage('Build'){
            steps{
            sh 'npm run build'
            }
        }
    }
}
